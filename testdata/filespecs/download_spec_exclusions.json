{
    "files": [
      {
        "pattern": "${REPO1}/data/testdata/a/",
        "target": "out/download/aql_by_artifacts/data/",
        "recursive": "false",
        "flat": "true",
        "exclusions": ["*/a1.in", "*a3.in", "*c2.in"]
      },
      {
        "pattern": "${REPO1}/data/testdata/a/b/",
        "target": "out/download/aql_by_artifacts/data/",
        "recursive": "true",
        "flat": "true",
        "exclusions": ["${REPO1}/data/testdata/a/*/*"]
      },
      {
        "pattern": "${REPO1}/data/testdata/a/b/c/",
        "target": "out/download/aql_by_artifacts/data/",
        "recursive": "false",
        "flat": "true",
        "exclusions": ["${REPO1}/data/testdata/a/b/c/c2.i?"]
      },
      {
        "pattern": "${REPO1}/data/testdata/a/b/",
        "target": "out/download/aql_by_artifacts/data/",
        "recursive": "true",
        "flat": "true",
        "exclusions": ["${REPO1}/*c*"]
      }
    ]
  }